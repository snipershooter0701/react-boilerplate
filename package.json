{
  "name": "cra-template-react-boilerplate",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/can-sahin/cra-template-react-boilerplate.git"
  },
  "license": "MIT",
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "bugs": {
    "url": "https://github.com/can-sahin/cra-template-react-boilerplate/issues"
  },
  "main": "template.json",
  "files": [
    "template",
    "template.json"
  ],
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "redux",
    "reduxjs",
    "react-redux",
    "react-boilerplate",
    "styled-components"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:all": "npm run test -- --watchAll=false",
    "test:coverage": "npm run test:all -- --coverage",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "npm run eslint -- src",
    "lint:fix": "npm run eslint -- --fix src",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "cleanExampleApp": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "start:prod": "npm run build && serve -s build",
    "publish:github": "cross-env PUBLIC_URL='/cra-template-react-boilerplate' npm run build && gh-pages -d build",
    "generate-cra": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/generate-cra.ts",
    "test:generators:cra": "cross-env TESTING_GENERATED_CRA=true npm run test:generators",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/tests/test-generators.ts",
    "generate-template": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/generate-template.ts",
    "test:cra": "npm run generate-cra && npm run test:generators:cra && cd generated-cra-app && npm run lint && npm run cleanExampleApp && npm run lint"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run checkTs"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/internals/",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*.test.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/styles/global-styles.ts",
      "!src/utils/@reduxjs/*",
      "!src/utils/redux-injectors.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    },
    "moduleNameMapper": {
      }
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.3.2",
    "connected-react-router": "6.8.0",
    "fontfaceobserver": "2.1.0",
    "history": "4.10.1",
    "i18next": "19.3.4",
    "i18next-browser-languagedetector": "4.0.2",
    "react": "16.13.0",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.0",
    "react-helmet-async": "1.0.4",
    "react-i18next": "11.3.4",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "11.0.0",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.1.1",
    "@testing-library/react": "10.0.1",
    "@types/fontfaceobserver": "0.0.6",
    "@types/history": "4.7.5",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.3",
    "@types/react-helmet": "5.0.15",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.7",
    "@types/styled-components": "5.0.1",
    "@types/testing-library__jest-dom": "5.0.2",
    "@types/webpack": "4.41.8",
    "@types/webpack-env": "1.15.1",
    "chalk": "3.0.0",
    "coveralls": "3.0.11",
    "cross-env": "7.0.2",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "eslint-plugin-redux-saga": "1.1.3",
    "gh-pages": "2.2.0",
    "husky": "4.2.3",
    "jest-styled-components": "7.0.2",
    "jest-watch-typeahead": "0.4.2",
    "lint-staged": "10.0.8",
    "node-plop": "0.25.0",
    "plop": "2.6.0",
    "prettier": "2.0.1",
    "react-scripts": "3.4.1",
    "react-test-renderer": "16.13.0",
    "rimraf": "3.0.2",
    "serve": "11.3.0",
    "shelljs": "0.8.3",
    "style-loader": "1.1.3",
    "stylelint": "13.2.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "8.8.2",
    "typescript": "3.8.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
